{
  "aioConnectorMetadataSchemaVersion": "1.0-preview",
  "name": "RestConnector",
  "description": "Connector for polling a REST server for information",
  "version": "1.0.0",
  "imageConfigurationSettings": {
    "containerRegistryName": "contoso-acr.io",
    "imageName": "RestConnector",
    "tag": "1.0.0"
  },
  "supportedArchitectures": ["linux/amd64"],
  "inboundEndpoints": [
    {
      "endpointType": "Microsoft.Http:v1",
      "version": "2.0",
      "configSchemas": {
        "datasetConfigSchema": {
          "schema": {
            "$schema": "http://json-schema.org/draft-07/schema#",
            "title": "Dataset Config Schema",
            "type": "object",
            "properties": {
              "SamplingInterval": {
                "description": "How frequently to sample each dataset by default (in milliseconds)",
                "type": "integer"
              }
            }
          }
        }
      },
      "isEndpointTlsEnabled" : true
    }
  ]
}